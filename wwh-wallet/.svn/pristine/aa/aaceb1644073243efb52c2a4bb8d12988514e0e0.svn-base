                            iNow = 5;
                        }
                        paArray.eq(iNow).siblings().stop().animate({
                            opacity: 0
                        }, 500);
                        paArray.eq(iNow).stop().animate({
                            opacity: 1
                        }, 500);
                        paArray.eq(iNow).css('z-index', '2').siblings().css('z-index', '1');
                    });
                    rightBtn.click(function () {
                        iNow++;
                        if (iNow > 5) {
                            iNow = 0;
                        }
                        paArray.eq(iNow).siblings().stop().animate({
                            opacity: 0
                        }, 500);
                        paArray.eq(iNow).stop().animate({
                            opacity: 1
                        }, 500);
                        paArray.eq(iNow).css('z-index', '2').siblings().css('z-index', '1');
                    });
                });
