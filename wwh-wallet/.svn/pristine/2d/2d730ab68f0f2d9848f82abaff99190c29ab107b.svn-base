package com.wwh.vo;

/**
 * 
 * @ClassName: RecommendPointBStatisticsVO
 * @Description: A状态推荐点统计表
 * @author: YuZihao
 * @date: 2016年11月12日 下午5:36:45
 */
public class RecommendAPointStatisticsVO extends BaseVO {

	/**
	 * @fieldName: serialVersionUID
	 * @fieldType: long
	 * @Description: TODO
	 */
	private static final long serialVersionUID = -9112494951146181395L;
	/**
	 * 推荐点总数
	 */
	private Integer totalRecommendPoint = 0;
	/**
	 * 已使用推荐点总数
	 */
	private Integer useUpRecommendPoint = 0;
	/**
	 * 可使用推荐点剩余总数
	 */
	private Integer remainRecommendPoint = 0;

	/**
	 * 系统类型
	 */
	private String diskType;
	/**
	 * 是否 是减的操作 'Y' 'N'
	 */
	private Character reduce;

	public Character getReduce() {
		return reduce;
	}

	public void setReduce(Character reduce) {
		this.reduce = reduce;
	}

	public Integer getTotalRecommendPoint() {
		return totalRecommendPoint;
	}

	public void setTotalRecommendPoint(Integer totalRecommendPoint) {
		this.totalRecommendPoint = totalRecommendPoint;
	}

	public Integer getUseUpRecommendPoint() {
		return useUpRecommendPoint;
	}

	public void setUseUpRecommendPoint(Integer useUpRecommendPoint) {
		this.useUpRecommendPoint = useUpRecommendPoint;
	}

	public Integer getRemainRecommendPoint() {
		return remainRecommendPoint;
	}

	public void setRemainRecommendPoint(Integer remainRecommendPoint) {
		this.remainRecommendPoint = remainRecommendPoint;
	}

	public String getDiskType() {
		return diskType;
	}

	public void setDiskType(String diskType) {
		this.diskType = diskType;
	}

}
