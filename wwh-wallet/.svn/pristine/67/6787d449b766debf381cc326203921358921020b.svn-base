package com.wwh.vo;

import java.math.BigDecimal;

/**
 * 
 * @ClassName: WalletAmountVO
 * @Description: 钱包表
 * @author: yuzih
 * @date: 2016年11月5日 下午6:50:18
 */
public class WalletAmountVO extends BaseVO {
	/**
	 * @fieldName: serialVersionUID
	 * @fieldType: long
	 * @Description: TODO
	 */
	private static final long serialVersionUID = 8056285355910513060L;
	/**
	 * 总积分
	 */
	private BigDecimal totalScore = new BigDecimal(0);
	/**
	 * 充值金额
	 */
	private BigDecimal rechargeAmount = new BigDecimal(0);
	/**
	 * 平台收益总金额
	 */
	private BigDecimal platformTotalAmount = new BigDecimal(0);
	/**
	 * 使用积分
	 */
	private BigDecimal usedScore = new BigDecimal(0);
	/**
	 * 剩余积分
	 */
	private BigDecimal remainScore = new BigDecimal(0);

	public BigDecimal getTotalScore() {
		return totalScore;
	}

	public void setTotalScore(BigDecimal totalScore) {
		this.totalScore = totalScore;
	}

	public BigDecimal getRechargeAmount() {
		return rechargeAmount;
	}

	public void setRechargeAmount(BigDecimal rechargeAmount) {
		this.rechargeAmount = rechargeAmount;
	}

	public BigDecimal getUsedScore() {
		return usedScore;
	}

	public void setUsedScore(BigDecimal usedScore) {
		this.usedScore = usedScore;
	}

	public BigDecimal getRemainScore() {
		return remainScore;
	}

	public void setRemainScore(BigDecimal remainScore) {
		this.remainScore = remainScore;
	}

	public BigDecimal getPlatformTotalAmount() {
		return platformTotalAmount;
	}

	public void setPlatformTotalAmount(BigDecimal platformTotalAmount) {
		this.platformTotalAmount = platformTotalAmount;
	}

	@Override
	public String toString() {
		return "WalletAmountVO [totalScore=" + totalScore + ", rechargeAmount=" + rechargeAmount
				+ ", platformTotalAmount=" + platformTotalAmount + ", usedScore=" + usedScore + ", remainScore="
				+ remainScore + "]";
	}
}
