<script type="text/javascript">
        $(document).ready(function () {
            $(function () {
                $(window).scroll(function () {
                    if ($(window).scrollTop() > 100) {
                        $("#back-to-top").fadeIn(500);
                    } else {
                        $("#back-to-top").fadeOut(500);
                    }
                });
                $("#back-to-top").click(function () {
                    $('body,html').animate({
                        scrollTop: 0
                    }, 100);
                    return false;
                });
            });
        });
</script>